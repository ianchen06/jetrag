{
  "containerDefinitions": [
    {
      "name": "jetrag3",
      "image": "068993006585.dkr.ecr.ap-northeast-1.amazonaws.com/jetrag3-crawler:1659420092",
      "cpu": 0,
      "portMappings": [],
      "essential": true,
      "environment": [
        {
          "name": "JETRAG_ENV",
          "value": "dev"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "RDS_AWS_ACCESS_KEY_ID",
          "valueFrom": "/ecs/jetrag3-crawler-dev/rds_aws_access_key_id"
        },
        {
          "name": "RDS_AWS_SECRET_ACCESS_KEY",
          "valueFrom": "/ecs/jetrag3-crawler-dev/rds_aws_secret_access_key"
        },
        {
          "name": "SLACK_WEBHOOK_URL",
          "valueFrom": "/ecs/jetrag3-crawler-dev/slack_webhook_url"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/jetrag3-crawler-dev",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "jetrag3-crawler-dev",
  "taskRoleArn": "arn:aws:iam::068993006585:role/jetrag3-IAM-Role-ECS",
  "executionRoleArn": "arn:aws:iam::068993006585:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
